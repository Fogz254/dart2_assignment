// Dart program demonstrating data types

void main() {
  // int: Represents integer values
  int integerValue = 42;
  print('Integer Value: $integerValue');

  // double: Represents double-precision floating-point numbers
  double doubleValue = 3.14;
  print('Double Value: $doubleValue');

  // String: Represents sequences of characters
  String stringValue = 'Hello, Dart!';
  print('String Value: $stringValue');

  // List: Represents an ordered collection of elements
  List<int> intList = [1, 2, 3, 4, 5];
  print('List of Integers: $intList');

  // Map: Represents a collection of key-value pairs
  Map<String, int> fruitPrices = {'Apple': 2, 'Banana': 1, 'Orange': 3};
  print('Fruit Prices: $fruitPrices');

  // Testing for performance with a loop
  for (int i = 0; i < 5; i++) {
    print('Loop iteration: $i');
  }
}

